//ONLY USED WHEN IN DEVELOPMENT!!

{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:4000",
      "sslPort": 44308 //do not edit
    }
  },
  "profiles": {
    "BookTrackers2000": { //used if application is run using .NET Core CLI
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "api/BookTrackers",
      "applicationUrl": "https://localhost:4000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "IIS Express": { //used if application is run from Visual Studio (button etc)
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "api/BookTrackers",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}
